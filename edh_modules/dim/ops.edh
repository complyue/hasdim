{##
 # Resemble Numpy vectorized operations, with simplified `ufunc`s
 #}


export {

  import * 'dim/primitive/ops'

  method sum( col ) fold( add, col )
  method nansum( col ) fold( add'valid, col )

  method cumsum( col ) scan( add, col )
  method nancumsum( col ) scan( add'valid, col )


  method prod( col ) fold( multiply, col )
  method nanprod( col ) fold( multiply'valid, col )

  method cumprod( col ) scan( multiply, col )
  method nancumprod( col ) scan( multiply'valid, col )

}
